
.nullBtn {
	background: none;
	padding: 0;
	text-align: left;
}
.nullBtn:after {
	border: 0;
}
.page {
	width: 100vw;
	height: 100vh;
}
.posterImg {
	width: 100%;
}
.settingBtn {
	position: fixed;
	right: 5%;
	bottom: 10%;
	width: 80rpx;
	height: 80rpx;
	z-index: 100;
	border: 1px solid #999;
	padding: 12rpx;
	box-sizing: border-box;
	border-radius: 50%;
}
.settingBtn image {
	width: 100%;
	height: 100%;
}

