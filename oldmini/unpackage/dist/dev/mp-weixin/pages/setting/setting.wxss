
.page {}
.showTemplate {
	position: fixed;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	background: #fff;
	z-index: 100;
	padding-top: calc(25px + 60px);
}
.forms {
	padding: 30rpx;
	padding-bottom: 110rpx;
	box-sizing: border-box;
}
.templateBox {
	position: relative;
	border: 1px solid #ccc;
	padding: 30rpx;
	box-sizing: border-box;
	border-radius: 20rpx;
	margin: 30rpx 0;
	padding-bottom: 80rpx;
}
.btn2 {
	width: 100%;
	position: absolute;
	left: 0;
	bottom: 0;
	overflow: hidden;
	z-index: 100;
	border-radius: 0 0 20rpx 20rpx;
	overflow: hidden;
}
.btn2 button {
	width: 50%;
	float: left;
	height: 66rpx;
	line-height: 66rpx;
	font-size: 26rpx;
}
.btn {
	position: fixed;
	left: 0;
	bottom: 0;
	width: 100%;
	overflow: hidden;
	z-index: 100;
}
.btn button {
	width: 50%;
	float: left;
}
.submit {
	position: fixed;
	left: 0;
	bottom: 0;
	width: 100%;
	border-radius: 0;
}

